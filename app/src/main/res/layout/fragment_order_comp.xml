<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cl_ordercompFm_05"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="market.OrderCompFragment" >

    <TextView
        android:id="@+id/tv_ordercompFm_05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/textViewOrderComp"
        android:textColor="@color/textcolor"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="訂單完成" />

    <Button
        android:id="@+id/bt_ordercompFm_05"
        android:layout_width="415dp"
        android:layout_height="wrap_content"
        android:text="@string/bottonConfirm"
        android:textSize="25sp"
        app:backgroundTint="@color/maincolor"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        tools:text="確認" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv_ordercompFm_05"
        android:layout_width="match_parent"
        android:layout_height="480dp"
        android:layout_marginTop="10dp"
        app:layout_constraintBottom_toTopOf="@+id/bt_ordercompFm_05"
        app:layout_constraintTop_toBottomOf="@+id/tv_ordercompFm_05" />
</androidx.constraintlayout.widget.ConstraintLayout>