<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cl_ordercompFm_05"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="market.OrderCompFragment" >

    <TextView
        android:id="@+id/tv_OrderComp_05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/textViewOrderComp"
        android:textColor="@color/textcolor"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="訂單完成" />

    <TextView
        android:id="@+id/tv_OrderCompAddress_05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="60dp"
        android:layout_marginLeft="60dp"
        android:text="@string/textViewTransAddress"
        android:textColor="@color/textcolor"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView3"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView3"
        tools:text="運送地址" />

    <TextView
        android:id="@+id/tv_OrderCompAddress1_05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:textColor="@color/textcolor"
        android:textSize="22sp"
        app:layout_constraintStart_toStartOf="@+id/tv_OrderCompAddress_05"
        app:layout_constraintTop_toBottomOf="@+id/tv_OrderCompAddress_05"
        tools:text="新北市三重區" />

    <TextView
        android:id="@+id/tv_OrderCompPay_05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="60dp"
        android:layout_marginLeft="60dp"
        android:textColor="@color/textcolor"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView7"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView7"
        tools:text="付款方式      Google Pay" />

    <TextView
        android:id="@+id/tv_OrderCompOrderNum_05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="@string/textViewOrderNumber"
        android:textColor="@color/textcolor"
        android:textSize="22sp"
        app:layout_constraintStart_toStartOf="@+id/tv_OrderCompPay_05"
        app:layout_constraintTop_toBottomOf="@+id/tv_OrderCompPay_05"
        tools:text="訂單編碼" />

    <TextView
        android:id="@+id/tv_OrderCompOrderTime_05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="@string/textViewOrderTime"
        android:textColor="@color/textcolor"
        android:textSize="22sp"
        app:layout_constraintStart_toStartOf="@+id/tv_OrderCompOrderNum_05"
        app:layout_constraintTop_toBottomOf="@+id/tv_OrderCompOrderNum_05"
        tools:text="訂單時間" />

    <TextView
        android:id="@+id/tv_OrderCompOrderNum1_05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginLeft="50dp"
        android:textColor="@color/textcolor"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="@+id/tv_OrderCompOrderNum_05"
        app:layout_constraintStart_toEndOf="@+id/tv_OrderCompOrderNum_05"
        app:layout_constraintTop_toTopOf="@+id/tv_OrderCompOrderNum_05"
        tools:text="1233244242" />

    <TextView
        android:id="@+id/tv_OrderCompOrderTime1_05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginLeft="48dp"
        android:layout_marginTop="20dp"
        android:textColor="@color/textcolor"
        android:textSize="22sp"
        app:layout_constraintStart_toEndOf="@+id/tv_OrderCompOrderNum_05"
        app:layout_constraintTop_toBottomOf="@+id/tv_OrderCompOrderNum1_05"
        tools:text="1233244242" />

    <Button
        android:id="@+id/bt_ordercompFm_05"
        android:layout_width="415dp"
        android:layout_height="wrap_content"
        android:text="@string/bottonConfirm"
        android:textSize="25sp"
        app:backgroundTint="@color/maincolor"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        tools:text="確認" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv_ordercompFm_05"
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_OrderComp_05"
        tools:layout_editor_absoluteX="-3dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="33dp"
        android:layout_height="33dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/icon_location_05"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rv_ordercompFm_05" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="29dp"
        android:layout_height="31dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/icon_money_05"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_OrderCompAddress1_05" />

</androidx.constraintlayout.widget.ConstraintLayout>