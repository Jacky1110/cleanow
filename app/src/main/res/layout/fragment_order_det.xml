<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cl_ordercomplete_05"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="market.OrderDetFragment">

    <Button
        android:id="@+id/bt_shoplistBuyFm_"
        android:layout_width="200sp"
        android:layout_height="wrap_content"
        android:text="@string/bottonBuy"
        android:textSize="25sp"
        app:backgroundTint="@color/paycolor"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        tools:text="結帳" />

    <TextView
        android:id="@+id/tv_orderdetFm_05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/textViewOrdetdet"
        android:textColor="@color/textcolor"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="訂單詳情" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv_orderdetFm_05"
        android:layout_width="match_parent"
        android:layout_height="475dp"
        android:layout_marginTop="25dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_orderdetFm_05"
        tools:layout_editor_absoluteX="16dp" />
</androidx.constraintlayout.widget.ConstraintLayout>