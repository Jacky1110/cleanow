<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cl_cardinfoFm_05"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="market.CardInfoFragment">

    <Button
        android:id="@+id/bt_cardinfoFm_05"
        android:layout_width="415sp"
        android:layout_height="wrap_content"
        android:text="@string/bottonConfirm"
        android:textSize="25sp"
        app:backgroundTint="@color/maincolor"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        tools:text="確認" />

    <CheckBox
        android:id="@+id/cb_cardinfoSeller_05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="30dp"
        android:text="@string/checkboxIagree"
        android:textColor="#4E4F4C"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_cardinfoMust3Fm_05"
        tools:text="我同意記錄本次付款資訊，並設定為快速付款" />

    <TextView
        android:id="@+id/tv_cardinfoSelectFm_05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:textColor="@color/textcolor"
        android:textSize="18sp"
        app:layout_constraintStart_toEndOf="@+id/iv_cardinfoShoppingFM_05"
        app:layout_constraintTop_toTopOf="@+id/iv_cardinfoShoppingFM_05"
        tools:text="目前選了2件商品" />

    <TextView
        android:id="@+id/tv_cardinfoFm_05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="40dp"
        android:text="@string/textViewCardInfo"
        android:textColor="@color/textcolor"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_cardinfoShoppingFM_05"
        tools:text="信用卡資訊" />

    <TextView
        android:id="@+id/tv_inputcardnumberFm_05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginLeft="15dp"
        android:text="@string/textViewEnterCardNumber"
        android:textColor="#4E4F4C"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/et_cardinfoEnterCn_05"
        app:layout_constraintStart_toStartOf="@+id/et_cardinfoEnterCn_05"
        app:layout_constraintTop_toTopOf="@+id/et_cardinfoEnterCn_05"
        tools:text="請輸入信用卡卡號" />

    <TextView
        android:id="@+id/tv_validmonFm_05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginLeft="15dp"
        android:text="@string/textViewValidMon"
        android:textColor="#4E4F4C"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/et_cardinfoValidM_05"
        app:layout_constraintStart_toStartOf="@+id/et_cardinfoValidM_05"
        app:layout_constraintTop_toTopOf="@+id/et_cardinfoValidM_05"
        tools:text="有效月" />

    <TextView
        android:id="@+id/tv_validyearFm_05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginLeft="15dp"
        android:text="@string/textViewValidYear"
        android:textColor="#4E4F4C"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/et_cardinfoValidY_05"
        app:layout_constraintStart_toStartOf="@+id/et_cardinfoValidY_05"
        app:layout_constraintTop_toTopOf="@+id/et_cardinfoValidY_05"
        app:layout_constraintVertical_bias="0.523"
        tools:text="有效年" />

    <TextView
        android:id="@+id/tv_lastnumberFm_05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginLeft="15dp"
        android:text="@string/textViewEnterLastNumber"
        android:textColor="#4E4F4C"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/et_cardinfoValidLast_05"
        app:layout_constraintStart_toStartOf="@+id/et_cardinfoValidLast_05"
        app:layout_constraintTop_toTopOf="@+id/et_cardinfoValidLast_05"
        tools:text="輸入未三碼" />

    <TextView
        android:id="@+id/tv_cardinfoMoneyFm_05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="5dp"
        android:textColor="@color/textcolor"
        android:textSize="18sp"
        app:layout_constraintStart_toEndOf="@+id/iv_cardinfoShoppingFM_05"
        app:layout_constraintTop_toBottomOf="@+id/tv_cardinfoSelectFm_05"
        tools:text="總金額 : $ 150" />

    <ImageView
        android:id="@+id/iv_cardinfoShoppingFM_05"
        android:layout_width="60dp"
        android:layout_height="51dp"
        android:layout_marginStart="77dp"
        android:layout_marginLeft="77dp"
        android:layout_marginTop="25dp"
        android:src="@drawable/icon_shopping_05"
        android:tint="@color/maincolor"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/iv_cardinfoMust1Fm_05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="35dp"
        android:src="@drawable/icon_asset_05"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_cardinfoFm_05" />

    <ImageView
        android:id="@+id/iv_cardinfoMust2Fm_05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="30dp"
        android:src="@drawable/icon_asset_05"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_cardinfoMust1Fm_05" />

    <ImageView
        android:id="@+id/iv_cardinfoMust3Fm_05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="30dp"
        android:src="@drawable/icon_asset_05"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_cardinfoMust2Fm_05" />

    <EditText
        android:id="@+id/et_cardinfoEnterCn_05"
        android:layout_width="320dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="@+id/iv_cardinfoMust1Fm_05"
        app:layout_constraintStart_toEndOf="@+id/iv_cardinfoMust1Fm_05"
        app:layout_constraintTop_toTopOf="@+id/iv_cardinfoMust1Fm_05" />

    <EditText
        android:id="@+id/et_cardinfoValidM_05"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="@+id/iv_cardinfoMust2Fm_05"
        app:layout_constraintStart_toEndOf="@+id/iv_cardinfoMust1Fm_05"
        app:layout_constraintTop_toTopOf="@+id/iv_cardinfoMust2Fm_05" />

    <EditText
        android:id="@+id/et_cardinfoValidLast_05"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="@+id/iv_cardinfoMust3Fm_05"
        app:layout_constraintStart_toEndOf="@+id/iv_cardinfoMust3Fm_05"
        app:layout_constraintTop_toTopOf="@+id/iv_cardinfoMust3Fm_05" />

    <EditText
        android:id="@+id/et_cardinfoValidY_05"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="@+id/et_cardinfoValidM_05"
        app:layout_constraintTop_toTopOf="@+id/et_cardinfoValidM_05"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_editor_absoluteX="210dp" />

    <ImageView
        android:id="@+id/iv_cardinfoGoin_05"
        android:layout_width="25dp"
        android:layout_height="23dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:src="@drawable/icon_enter_05"
        app:layout_constraintBottom_toBottomOf="@+id/et_cardinfoEnterCn_05"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/et_cardinfoEnterCn_05" />

</androidx.constraintlayout.widget.ConstraintLayout>